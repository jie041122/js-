// 创建数组
const arr=[1,121,32,54]
const arr2=new Array()      
// 添加数组
arr2[0]=65
// 在数组后面添加数组
arr2[arr2.length]=79
// 修改length长度
arr2.lenght=5
// 获取索引长度
 console[arr.length]    
// 遍历数组
for(let i=0;i<arr.length;i++){
   console.log(arr[i])   
  }
for(let i=arr.length-1;i>=0;i--){
   console.log(arr[i])   
    }
for(value of arr){  
   console.log(value) 
   }
console.log(arr)
console.log(arr[2])
console.log(arr2)
